# Configurações do Banco de Dados (Railway fornece automaticamente)
DATABASE_URL=postgresql://usuario:senha@host:porta/banco

# Segredo para sessões (gere uma string aleatória)
SESSION_SECRET=sua-chave-secreta-aqui-mude-isso

# Ambiente
NODE_ENV=production

# Porta (Railway define automaticamente)
PORT=3000

# Chave de admin para acessar dados das usuárias (gere uma senha forte)
ADMIN_KEY=sua-chave-admin-secreta-aqui
